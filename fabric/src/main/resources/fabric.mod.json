{
    "schemaVersion": 1,
    "id": "mosaic_cosmetics",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/MosaicModding/MosaicCosmetics",
        "issues": "https://github.com/MosaicModding/MosaicCosmetics/issues"
    },
    "license": "${license}",
    "icon": "assets/modid/${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.mosaic_modding.mosaic_cosmetics.MosaicCosmeticsFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "accessWidener" : "mosaic_cosmetics.accesswidener",
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  